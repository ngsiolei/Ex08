<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage x:Class="Ex08.Views.PageB"
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:views="clr-namespace:Ex08.Views"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ContentPage.Content>
        <StackLayout>
            <views:NavBar Margin="0,25,0,0" />
            <Button BackgroundColor="#EEE"
                    Command="{Binding NavigateCommand}"
                    CommandParameter="PageA"
                    HorizontalOptions="Center"
                    Margin="0,30,0,0"
                    Text="Go to Page A"
                    WidthRequest="200" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>